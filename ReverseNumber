public class Program
{
	public static void Main()
	{

	//Num is the number that will be reversed

	int Num = 123456789;

	//run a loop as long as the number is higher than 0

	while(Num > 0)
		{
    	
    	//divide the number by 10 with a remainder

    	int newNum = Num % 10;
    	Num = Num / 10;

    	//write the remainder to the console

		Console.Write(newNum);
		}
	}
}